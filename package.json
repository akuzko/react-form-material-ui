{
  "name": "react-form-material-ui",
  "version": "1.2.0",
  "description": "material-ui input bindings for react-form-base",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clear": "rimraf lib",
    "rebuild": "npm run clear && npm run build",
    "lint": "./node_modules/.bin/eslint src test --ext .js --ext .jsx",
    "test": "mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akuzko/react-form-material-ui.git"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "react",
    "reactjs",
    "react-components",
    "form",
    "material-ui"
  ],
  "author": "Artem Kuzko <a.kuzko@gmail.com> (https://github.com/akuzko)",
  "license": "MIT",
  "dependencies": {
    "react-form-base": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-root-import": "^5.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "expect": "^1.20.2",
    "jsdom": "^9.10.0",
    "material-ui": "^0.16.0 || ^0.17.0",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tap-event-plugin": "^2.0.1",
    "rimraf": "^2.5.4",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "react": ">= 15.0.0",
    "material-ui": ">= 0.16.0",
    "react-tap-event-plugin": ">= 1.0.0"
  }
}
